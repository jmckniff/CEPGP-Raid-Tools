
@{
    ViewBag.Title = "Index";
}

<h2>Upload CEPGP Transactions</h2>

@using (Html.BeginForm("Upload", "Transactions", FormMethod.Post, new { @class = "form", enctype = "multipart/form-data" }))
{
    <div class="row">
        <div class="col-md-12" style="margin-bottom: 20px;">
            <input type="file" name="file" />
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <input type="submit" name="Submit" id="Submit" value="Upload" />
        </div>
    </div>
}
